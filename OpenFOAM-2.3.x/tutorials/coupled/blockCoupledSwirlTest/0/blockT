FoamFile
{
    version     2.0;
    format      ascii;
    class       volVector2Field;
    location    "0";
    object      blockT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (300 300);

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform (0 0);
    }
    outlet
    {
        type            calculated;
        value           uniform (0 0);
    }
    fixedWalls
    {
        type            calculated;
        value           uniform (0 0);
    }
    defaultFaces
    {
        type            empty;
    }
}


