FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

4
(
    inlet
    {
        type            patch;
        nFaces          20;
        startFace       1540;
    }
    outlet
    {
        type            patch;
        nFaces          20;
        startFace       1560;
    }
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          80;
        startFace       1580;
    }
    defaultFaces
    {
        type            empty;
        inGroups        1(empty);
        nFaces          1600;
        startFace       1660;
    }
)

